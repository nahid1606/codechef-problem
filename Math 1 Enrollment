#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--){
	    int x,y;
	    scanf("%d %d",&x,&y);
	    if(x==y){
	        printf("0\n");
	    }
	    else if(x>y){
	        printf("0\n",x-y);
	    }
	    else if(x<y){
	        printf("%d\n",-x+y);
	    }
	    else
	    {
	        printf("0\n");
	    }
	}

}
